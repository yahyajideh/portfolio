    "@emotion/styled": "^11.1.5",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.1.2",
    "gatsby-mdx": "^1.0.0",
    "gatsby-plugin-emotion": "^6.1.0",
    "gatsby-plugin-image": "^2.10.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-sharp": "^4.10.1",
    "gatsby-source-filesystem": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
